{
  "expo": {
    "name": "OpenClaw Controller",
    "slug": "openclaw-controller",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dhruvkelawala.openclaw-controller",
      "infoPlist": {
        "NSFaceIDUsageDescription": "This app uses Face ID to authenticate approval actions."
        // Push notifications disabled for local testing without Apple Developer Program
        // "UIBackgroundModes": ["remote-notification"]
      }
    },
    "plugins": [
      "expo-router",
      "expo-dev-client"
      // Push notifications plugin disabled for local testing without Apple Developer Program
      // [
      //   "expo-notifications",
      //   {
      //     "icon": "./assets/notification-icon.png",
      //     "color": "#ffffff",
      //     "defaultChannel": "default",
      //     "sounds": [
      //       "./assets/sumo-notification-kimi.wav",
      //       "./assets/sumo-notification-opus.wav",
      //       "./assets/sumo-notification-codex.wav"
      //     ]
      //   }
      // ]
    ],
    "scheme": "openclaw",
    "extra": {
      "router": {
        "origin": false
      },
      "backendUrl": "https://openclaw-prod.tailbc93c6.ts.net",
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}
